@prefix exsrs: <https://w3id.org/example-data-srs#> .
@prefix geosrs_cs: <https://w3id.org/geosrs/cs/> .
@prefix om: <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

exsrs:ecsc a geosrs_cs:CartesianCoordinateSystem ;
    rdfs:label "My example cartesian coordinate system with two axis" ;
    geosrs_cs:axis exsrs:ecsc_axis1,
        exsrs:excsc_axis2 .

exsrs:ecsc_axis2 a geosrs_cs:Axis ;
    rdfs:label "Easting" ;
    om:hasUnit om:metre ;
    skos:altLabel "E" ;
    geosrs_cs:axisDirection geosrs_cs:East .

exsrs:ecsc_axis1 a geosrs_cs:Axis ;
    rdfs:label "Northing" ;
    om:hasUnit om:metre ;
    skos:altLabel "N" ;
    geosrs_cs:axisDirection geosrs_cs:North .

