@prefix geosrs: <https://w3id.org/geosrs/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

geosrs:CRSShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S1"@en,
        "CRS Ontology Shape S2"@en,
        "CRS Ontology Shape S3"@en,
        "CRS Ontology Shape S4"@en ;
    sh:property geosrs:CRSShape_Property ;
    sh:targetNode geosrs:CRS .

geosrs:CompoundCRSShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S5"@en ;
    sh:property geosrs:CompoundCRSShape_Property ;
    sh:targetNode geosrs:CompoundCRS .

geosrs:GeodeticCRSShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S6"@en ;
    sh:property geosrs:GeodeticCRSShape_Property ;
    sh:targetNode geosrs:GeodeticCRS .

geosrs:GeographicCRSShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S7"@en,
        "CRS Ontology Shape S8"@en ;
    sh:property geosrs:GeographicCRSShape_Property ;
    sh:targetNode geosrs:GeographicCRS .

geosrs:ParametricCRSShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S9"@en ;
    sh:property geosrs:ParametricCRSShape_Property ;
    sh:targetNode geosrs:ParametricCRS .

geosrs:ProjectedCRSShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S10"@en ;
    sh:property geosrs:ProjectedCRSShape_Property ;
    sh:targetNode geosrs:ProjectedCRS .

geosrs:SingleCRSShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S11"@en,
        "CRS Ontology Shape S12"@en ;
    sh:property geosrs:SingleCRSShape_Property ;
    sh:targetNode geosrs:SingleCRS .

geosrs:TemporalCRSShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S13"@en ;
    sh:property geosrs:TemporalCRSShape_Property ;
    sh:targetNode geosrs:TemporalCRS .

geosrs:CRSShape_Property sh:class geosrs:AreaOfUse,
        geosrs:CoordinateSystem,
        geosrs:Datum,
        geosrs:DatumEnsemble ;
    sh:maxCount 1 ;
    sh:message "A coordinate reference system may have exactly one datum ensemble"@en,
        "A coordinate reference system should have at least one area of use"@en,
        "A coordinate reference system should have exactly one coordinate system"@en,
        "A coordinate reference system should have exactly one datum"@en ;
    sh:minCount 1 .

geosrs:CompoundCRSShape_Property sh:class geosrs:SingleCRS ;
    sh:message "A compound coordinate reference system should consist of at least one single coordinate reference system"@en ;
    sh:minCount 1 .

geosrs:GeodeticCRSShape_Property sh:class geosrs:GeodeticCoordinateSystem ;
    sh:maxCount 1 ;
    sh:message "A geodetic coordinate reference system should have exactly one geodetic coordinate system"@en ;
    sh:minCount 1 .

geosrs:GeographicCRSShape_Property sh:class geosrs:EllipsoidalCoordinateSystem,
        geosrs:GeodeticDatum ;
    sh:maxCount 1 ;
    sh:message "A geographic coordinate reference system should have exactly one ellipsoidal coordinate system"@en,
        "A geographic coordinate reference system should have exactly one geodetic datum"@en ;
    sh:minCount 1 .

geosrs:ParametricCRSShape_Property sh:class geosrs:ParametricDatum ;
    sh:maxCount 1 ;
    sh:message "A parametric coordinate reference system should have exactly one parametric datum"@en ;
    sh:minCount 1 .

geosrs:ProjectedCRSShape_Property sh:class geosrs:Conversion ;
    sh:message "A projected coordinate reference system should have at least one conversion"@en ;
    sh:minCount 1 .

geosrs:SingleCRSShape_Property sh:class geosrs:CoordinateSystem,
        geosrs:Datum ;
    sh:maxCount 1 ;
    sh:message "A single coordinate reference system should have exactly one coordinate system"@en,
        "A single coordinate reference system should have exactly one datum"@en ;
    sh:minCount 1 .

geosrs:TemporalCRSShape_Property sh:class geosrs:TemporalDatum ;
    sh:maxCount 1 ;
    sh:message "A projected coordinate reference system should have exactly one temporal datum"@en ;
    sh:minCount 1 .

