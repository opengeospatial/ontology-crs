geosrs:CRS_coordinateSystem_Shape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S1"@en ;
    sh:property geosrs:CRS_coordinateSystem_Shape_Property ;
    sh:targetNode geosrs:CRS .

