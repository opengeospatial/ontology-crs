@prefix geosrs: <http://www.opengis.net/ont/srs/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

geosrs:CoordinateOperation a owl:Class ;
    rdfs:label "coordinate operation"@en .

geosrs:PrimeMeridian a owl:Class ;
    rdfs:label "Prime meridian"@en .

geosrs:axis a owl:ObjectProperty ;
    rdfs:label "axis"@en ;
    rdfs:domain geosrs:CoordinateSystem ;
    rdfs:range geosrs:Axis ;
    skos:definition "The property relates a coordinate system to one of its axis"@en .

geosrs:axisDirection a owl:ObjectProperty ;
    rdfs:label "axisdirection"@en ;
    rdfs:domain geosrs:Axis ;
    rdfs:range geosrs:AxisDirection ;
    skos:definition "The property relates a coordinate system axis to its axis direction"@en .

geosrs:baseCRS a owl:ObjectProperty ;
    rdfs:label "base CRS"@en ;
    rdfs:domain geosrs:CRS ;
    rdfs:range geosrs:CRS ;
    skos:definition "The geodetic coordinate reference system on which a projected coordinate reference system is based. Cf. ISO 19111:2007:2007-07, table 11, association role baseCRS."@en .

geosrs:coordinateSystem a owl:ObjectProperty ;
    rdfs:label "coordinate system"@en ;
    rdfs:domain geosrs:CRS ;
    rdfs:range geosrs:CoordinateSystem ;
    skos:definition "The property relates a coordinate reference system to its coordinate system"@en .

geosrs:datum a owl:ObjectProperty ;
    rdfs:label "datum"@en ;
    rdfs:domain geosrs:CRS ;
    rdfs:range geosrs:Datum ;
    skos:definition "The property relates a coordinate reference system to a datum"@en .

geosrs:ellipsoid a owl:ObjectProperty ;
    rdfs:label "ellipsoid"@en ;
    rdfs:domain geosrs:Datum ;
    rdfs:range geosrs:Ellipsoid ;
    skos:definition "The properties relates a datum to its ellipsoid definition"@en .

geosrs:geosrs a owl:Ontology ;
    rdfs:label "SRS Ontology"@en ;
    vann:preferredNamespacePrefix "geosrs"^^xsd:string ;
    vann:preferredNamespaceUri "http://www.opengis.net/ont/srs/"^^xsd:anyURI .

geosrs:semiMajorAxis a owl:ObjectProperty ;
    rdfs:label "semi major axis"@en ;
    rdfs:domain geosrs:Ellipsoid ;
    rdfs:range geosrs:Axis ;
    skos:definition "Indicates the length of the semi major axis of an ellipsoid. Cf. ISO 19111:2007:2007-07, table 36, attribute length of semi-major axis."@en .

geosrs:semiMinorAxis a owl:ObjectProperty ;
    rdfs:label "semi minor axis"@en ;
    rdfs:domain geosrs:Ellipsoid ;
    rdfs:range geosrs:Axis ;
    skos:definition "Indicates the length of the semi minor axis of an ellipsoid. Cf. ISO 19111:2007:2007-07, table 37, attribute length of semi-minor axis."@en .

geosrs:sourceCRS a owl:ObjectProperty ;
    rdfs:label "source CRS"@en ;
    rdfs:domain geosrs:CRS ;
    rdfs:range geosrs:CRS ;
    skos:definition "The coordinate reference system associated to the data used as input of a given operation. Cf. ISO 19111:2007:2007-07, table 42, named association Source."@en .

geosrs:targetCRS a owl:ObjectProperty ;
    rdfs:label "target CRS"@en ;
    rdfs:domain geosrs:CRS ;
    rdfs:range geosrs:CRS ;
    skos:definition "The coordinate reference system associated to the data obtained as output of a given operation. Cf. ISO 19111:2007:2007-07, table 42, named association Target."@en .

geosrs:SpatialReferenceSystem a owl:Class ;
    rdfs:label "Spatial Reference System"@en ;
    rdfs:subClassOf geosrs:ReferenceSystem ;
    skos:definition "A reference system allowing the description of a position."@en .

geosrs:CoordinateSystem a owl:Class ;
    rdfs:label "coordinate system"@en ;
    skos:definition "Set of axes that spans a given coordinate space and of mathematical rules for specifying how coordinates are to be assigned to points. Cf. ISO 19111:2007:2007-07, part 9.2, table 17 and annex B.2."@en .

geosrs:Datum a owl:Class ;
    rdfs:label "Datum"@en .

geosrs:Ellipsoid a owl:Class ;
    rdfs:label "Ellipsoid"@en .

geosrs:CRS a owl:Class ;
    rdfs:label "Coordinate Reference System"@en ;
    rdfs:subClassOf geosrs:SpatialReferenceSystem ;
    skos:definition "Depending on the spatial dimension of coordinates (1D, 2D, 3D), this piece of metadata is used for specifying the elements of definition associated to a given set of coordinates: its datum, its ellipsoid, its prime meridian, the type of coordinates (geocentric, geographic, projected,...), the coordinates units of measure, when appropriate the cartographic projection used, the vertical coordinate reference system."@en .

