@prefix ns1: <https://w3id.org/geosrs/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.opengis.net/ont/crs/geod/Popular_Visualisation_Sphere> a <http://www.w3.org/ns/ldp#Resource>,
        ns1:Sphere,
        <https://www.iana.org/assignments/media-types/text/turtle#Resource> ;
    rdfs:label "Popular Visualisation Sphere"@en ;
    void:inDataset <http://www.opengis.net/geoepsg_dataset> ;
    skos:definition "+a=6378137.0 +f=0.0"^^xsd:string ;
    ns1:approximates <http://www.opengis.net/ont/crs/isbody/Earth> ;
    ns1:eccentricity 0e+00 ;
    ns1:flatteningParameter 0e+00 ;
    ns1:isSphere true ;
    ns1:semiMajorAxis "6378137.0"^^xsd:string ;
    ns1:semiMinorAxis "6378137.0"^^xsd:string .

