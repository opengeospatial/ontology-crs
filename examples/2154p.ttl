@prefix geosrs: <https://w3id.org/geosrs/geosrs/> .
@prefix geosrs_cs: <https://w3id.org/geosrs/cs/> .
@prefix om: <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

[] a geosrs:ProjectedCRS ;
    rdfs:label "RGF93 v1 / Lambert-93" ;
    skos:scopeNote "Engineering survey, topographic mapping." ;
    geosrs:baseCRS [ rdfs:label "RGF93 v1" ;
            geosrs:coordinateSystem [ a geosrs:EllipsoidalCoordinateSystem ;
                    geosrs:axis [ rdfs:label "Geodetic longitude" ;
                            om:hasUnit om:degree ;
                            skos:altLabel "Lon" ;
                            geosrs:axisDirection geosrs_cs:east ],
                        [ rdfs:label "Geodetic latitude" ;
                            om:hasUnit om:degree ;
                            skos:altLabel "Lat" ;
                            geosrs:axisDirection geosrs_cs:north ] ] ;
            geosrs:datum [ a geosrs:GeodeticDatum ;
                    rdfs:label "Reseau Geodesique Francais 1993 v1" ;
                    geosrs:ellipsoid [ rdfs:label "GRS 1980" ;
                            geosrs:inverseFlattening 2.982572e+02 ;
                            geosrs:semiMajorAxis 6378137 ] ] ] ;
    geosrs:conversion [ rdfs:label "Lambert-93" ;
            geosrs:method [ rdfs:label "Lambert Conic Conformal (2SP)" ] ;
            geosrs:operationParameter [ rdfs:label "Easting at false origin" ;
                    om:hasUnit om:metre ;
                    rdf:value 700000 ],
                [ rdfs:label "Northing at false origin" ;
                    om:hasUnit om:metre ;
                    rdf:value 6600000 ],
                [ rdfs:label "Latitude of false origin" ;
                    om:hasUnit om:degree ;
                    rdf:value 4.65e+01 ],
                [ rdfs:label "Longitude of false origin" ;
                    om:hasUnit om:degree ;
                    rdf:value 3 ],
                [ rdfs:label "Latitude of 2nd standard parallel" ;
                    om:hasUnit om:degree ;
                    rdf:value 44 ],
                [ rdfs:label "Latitude of 1st standard parallel" ;
                    om:hasUnit om:degree ;
                    rdf:value 49 ] ] ;
    geosrs:coordinateSystem [ a geosrs:CartesianCoordinateSystem ;
            geosrs:axis [ rdfs:label "Easting" ;
                    om:hasUnit om:metre ;
                    skos:altLabel "X" ;
                    geosrs:axisDirection geosrs_cs:east ],
                [ rdfs:label "Northing" ;
                    om:hasUnit om:metre ;
                    skos:altLabel "Y" ;
                    geosrs:axisDirection geosrs_cs:north ] ] .

