@prefix geosrs: <https://w3id.org/geosrs/geosrs#> .
@prefix om: <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

[] a <file:///home/runner/work/ontology-crs/ontology-crs/examples/ProjectedCRS> ;
    rdfs:label "ED50 / UTM zone 32N" ;
    skos:scopeNote "Engineering survey, topographic mapping." ;
    geosrs:baseCRS [ rdfs:label "ED50" ;
            geosrs:coordinateSystem [ a <file:///home/runner/work/ontology-crs/ontology-crs/examples/EllipsoidalCoordinateSystem> ;
                    geosrs:axis [ rdfs:label "Geodetic longitude" ;
                            om:hasUnit om:degree ;
                            skos:altLabel "Lon" ;
                            geosrs:axisDirection "east" ],
                        [ rdfs:label "Geodetic latitude" ;
                            om:hasUnit om:degree ;
                            skos:altLabel "Lat" ;
                            geosrs:axisDirection "north" ] ] ;
            geosrs:datum [ a geosrs:GeodeticDatum ;
                    rdfs:label "European Datum 1950" ;
                    geosrs:ellipsoid [ rdfs:label "International 1924" ;
                            geosrs:semiMajorAxis 6378388 ] ] ] ;
    geosrs:coordinateSystem [ a geosrs:CartesianCoordinateSystem ;
            geosrs:axis [ rdfs:label "Northing" ;
                    om:hasUnit om:metre ;
                    skos:altLabel "N" ;
                    geosrs:axisDirection "north" ],
                [ rdfs:label "Easting" ;
                    om:hasUnit om:metre ;
                    skos:altLabel "E" ;
                    geosrs:axisDirection "east" ] ] .

