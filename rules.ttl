@prefix geosrs: <https://w3id.org/geosrs/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

geosrs:CoordinateSystemShape a sh:NodeShape ;
    sh:property geosrs:CoordinateSystemShape_Property ;
    sh:targetNode geosrs:CoordinateSystem .

geosrs:CoordinateSystemShape_Property rdfs:comment "A coordinate system should have at least one axis"@en ;
    sh:class geosrs:Axis ;
    sh:minCount 1 .

