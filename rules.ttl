@prefix geosrs: <https://w3id.org/geosrs/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

geosrs:3DCoordinateSystemShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S1"@en ;
    sh:property geosrs:3DCoordinateSystemShape_Property ;
    sh:targetNode geosrs:3DCoordinateSystem .

geosrs:CRSShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S1"@en,
        "CRS Ontology Shape S2"@en,
        "CRS Ontology Shape S3"@en,
        "CRS Ontology Shape S4"@en ;
    sh:property geosrs:CRSShape_Property ;
    sh:targetNode geosrs:CRS .

geosrs:CompoundCRSShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S5"@en ;
    sh:property geosrs:CompoundCRSShape_Property ;
    sh:targetNode geosrs:CompoundCRS .

geosrs:ConicalCoordinateSystemShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S2"@en ;
    sh:property geosrs:ConicalCoordinateSystemShape_Property ;
    sh:targetNode geosrs:ConicalCoordinateSystem .

geosrs:CoordinateSystemAxisShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S4"@en ;
    sh:property geosrs:CoordinateSystemAxisShape_Property ;
    sh:targetNode geosrs:CoordinateSystemAxis .

geosrs:CoordinateSystemShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S3"@en ;
    sh:property geosrs:CoordinateSystemShape_Property ;
    sh:targetNode geosrs:CoordinateSystem .

geosrs:CylindricalCoordinateSystemShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S5"@en ;
    sh:property geosrs:CylindricalCoordinateSystemShape_Property ;
    sh:targetNode geosrs:CylindricalCoordinateSystem .

geosrs:DateTimeTemporalCoordinateSystemShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S6"@en ;
    sh:property geosrs:DateTimeTemporalCoordinateSystemShape_Property ;
    sh:targetNode geosrs:DateTimeTemporalCoordinateSystem .

geosrs:GeodeticCRSShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S6"@en ;
    sh:property geosrs:GeodeticCRSShape_Property ;
    sh:targetNode geosrs:GeodeticCRS .

geosrs:GeographicCRSShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S7"@en,
        "CRS Ontology Shape S8"@en ;
    sh:property geosrs:GeographicCRSShape_Property ;
    sh:targetNode geosrs:GeographicCRS .

geosrs:ParametricCRSShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S9"@en ;
    sh:property geosrs:ParametricCRSShape_Property ;
    sh:targetNode geosrs:ParametricCRS .

geosrs:ParametricDatumShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S1"@en ;
    sh:property geosrs:ParametricDatumShape_Property ;
    sh:targetNode geosrs:ParametricDatum .

geosrs:PlanarCoordinateSystemShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S7"@en ;
    sh:property geosrs:PlanarCoordinateSystemShape_Property ;
    sh:targetNode geosrs:PlanarCoordinateSystem .

geosrs:ProjectedCRSShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S10"@en ;
    sh:property geosrs:ProjectedCRSShape_Property ;
    sh:targetNode geosrs:ProjectedCRS .

geosrs:SingleCRSShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S11"@en,
        "CRS Ontology Shape S12"@en ;
    sh:property geosrs:SingleCRSShape_Property ;
    sh:targetNode geosrs:SingleCRS .

geosrs:TemporalCRSShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S13"@en ;
    sh:property geosrs:TemporalCRSShape_Property ;
    sh:targetNode geosrs:TemporalCRS .

geosrs:TemporalCoordinateSystemShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S8"@en ;
    sh:property geosrs:TemporalCoordinateSystemShape_Property ;
    sh:targetNode geosrs:TemporalCoordinateSystem .

geosrs:TemporalCountCoordinateSystemShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S9"@en ;
    sh:property geosrs:TemporalCountCoordinateSystemShape_Property ;
    sh:targetNode geosrs:TemporalCountCoordinateSystem .

geosrs:TemporalMeasureCoordinateSystemShape a sh:NodeShape ;
    rdfs:label "CRS Ontology Shape S10"@en ;
    sh:property geosrs:TemporalMeasureCoordinateSystemShape_Property ;
    sh:targetNode geosrs:TemporalMeasureCoordinateSystem .

geosrs:3DCoordinateSystemShape_Property rdfs:comment "A 3D coordinate system should have at least three axes"@en ;
    sh:class geosrs:CoordinateSystemAxis ;
    sh:minCount 3 .

geosrs:CRSShape_Property rdfs:comment "A coordinate reference system may have exactly one datum ensemble"@en,
        "A coordinate reference system should have at least one area of use"@en,
        "A coordinate reference system should have exactly one coordinate system"@en,
        "A coordinate reference system should have exactly one datum"@en ;
    sh:class geosrs:AreaOfUse,
        geosrs:CoordinateSystem,
        geosrs:Datum,
        geosrs:DatumEnsemble ;
    sh:maxCount 1 ;
    sh:minCount 1 .

geosrs:CompoundCRSShape_Property rdfs:comment "A compound coordinate reference system should consist of at least one single coordinate reference system"@en ;
    sh:class geosrs:SingleCRS ;
    sh:minCount 1 .

geosrs:ConicalCoordinateSystemShape_Property rdfs:comment "A conical coordinate system should have at least three axes"@en ;
    sh:class geosrs:CoordinateSystemAxis ;
    sh:minCount 3 .

geosrs:CoordinateSystemAxisShape_Property rdfs:comment "A coordinate system axis should have exactly one axis direction"@en ;
    sh:class geosrs:AxisDirection ;
    sh:maxCount 1 ;
    sh:minCount 1 .

geosrs:CoordinateSystemShape_Property rdfs:comment "A coordinate system should have at least one axis"@en ;
    sh:class geosrs:CoordinateSystemAxis ;
    sh:minCount 1 .

geosrs:CylindricalCoordinateSystemShape_Property rdfs:comment "A cylindrical coordinate system should have at least three axes"@en ;
    sh:class geosrs:CoordinateSystemAxis ;
    sh:minCount 3 .

geosrs:DateTimeTemporalCoordinateSystemShape_Property rdfs:comment "A date time temporal coordinate system should have exactly one axis"@en ;
    sh:class geosrs:CoordinateSystemAxis ;
    sh:maxCount 1 ;
    sh:minCount 1 .

geosrs:GeodeticCRSShape_Property rdfs:comment "A geodetic coordinate reference system should have exactly one geodetic coordinate system"@en ;
    sh:class geosrs:GeodeticCoordinateSystem ;
    sh:maxCount 1 ;
    sh:minCount 1 .

geosrs:GeographicCRSShape_Property rdfs:comment "A geographic coordinate reference system should have exactly one ellipsoidal coordinate system"@en,
        "A geographic coordinate reference system should have exactly one geodetic datum"@en ;
    sh:class geosrs:EllipsoidalCoordinateSystem,
        geosrs:GeodeticDatum ;
    sh:maxCount 1 ;
    sh:minCount 1 .

geosrs:ParametricCRSShape_Property rdfs:comment "A parametric coordinate reference system should have exactly one parametric datum"@en ;
    sh:class geosrs:ParametricDatum ;
    sh:maxCount 1 ;
    sh:minCount 1 .

geosrs:ParametricDatumShape_Property rdfs:comment "A parametric datum should have at least one defining parameter"@en ;
    sh:class geosrs:DefiningParameter ;
    sh:minCount 1 .

geosrs:PlanarCoordinateSystemShape_Property rdfs:comment "A planar coordinate system should have at least two axes"@en ;
    sh:class geosrs:CoordinateSystemAxis ;
    sh:minCount 2 .

geosrs:ProjectedCRSShape_Property rdfs:comment "A projected coordinate reference system should have at least one conversion"@en ;
    sh:class geosrs:Conversion ;
    sh:minCount 1 .

geosrs:SingleCRSShape_Property rdfs:comment "A single coordinate reference system should have exactly one coordinate system"@en,
        "A single coordinate reference system should have exactly one datum"@en ;
    sh:class geosrs:CoordinateSystem,
        geosrs:Datum ;
    sh:maxCount 1 ;
    sh:minCount 1 .

geosrs:TemporalCRSShape_Property rdfs:comment "A projected coordinate reference system should have exactly one temporal datum"@en ;
    sh:class geosrs:TemporalDatum ;
    sh:maxCount 1 ;
    sh:minCount 1 .

geosrs:TemporalCoordinateSystemShape_Property rdfs:comment "A temporal coordinate system should have exactly one axis"@en ;
    sh:class geosrs:CoordinateSystemAxis ;
    sh:maxCount 1 ;
    sh:minCount 1 .

geosrs:TemporalCountCoordinateSystemShape_Property rdfs:comment "A temporal count coordinate system should have exactly one axis"@en ;
    sh:class geosrs:CoordinateSystemAxis ;
    sh:maxCount 1 ;
    sh:minCount 1 .

geosrs:TemporalMeasureCoordinateSystemShape_Property rdfs:comment "A temporal measure coordinate system should have exactly one axis"@en ;
    sh:class geosrs:CoordinateSystemAxis ;
    sh:maxCount 1 ;
    sh:minCount 1 .

